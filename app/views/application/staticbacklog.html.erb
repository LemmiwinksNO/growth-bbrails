              <div class="box">
                <div class="box-header text-center">
                  <h2>Growth App</h2>
                </div>
                <div class="box-content scrollable">
                  <ul class="items not-doing">
                    <li class="item ellipsis not-doing">
                      <!-- <span class="avatar"><i class="fi-star"></i></span> -->
                      <span class="ticket-number">30</span>
                      <span class="status">not doing</span>
                      <span class="title">Get Michele a parrot!</span>
                      <span class="notdo-delete right">
                        <button class="secondary extra-tiny radius">
                          <i class="fi-trash"></i>
                        </button>
                      </span>
                      <span class="priority right high">high</span>
                    </li>
                    <li class="item ellipsis doing">
                      <!-- <span class="avatar"><i class="fi-star"></i></span> -->
                      <span class="ticket-number">30</span>
                      <span class="status">doing</span>
                      <span class="title">Get Michele a parrot!</span>
                      <span class="notdo-delete right">
                        <button class="secondary extra-tiny radius">
                          <i class="fi-trash"></i>
                        </button>
                      </span>
                      <span class="priority right medium">medium</span>
                    </li>
                    <li class="item ellipsis done">
                      <!-- <span class="avatar"><i class="fi-star"></i></span> -->
                      <span class="ticket-number">30</span>
                      <span class="status">done</span>
                      <span class="title">Get Michele a parrot!</span>
                      <span class="notdo-delete right">
                        <button class="secondary extra-tiny radius">
                          <i class="fi-trash"></i>
                        </button>
                      </span>
                      <span class="priority right low">low</span>
                    </li>
                  </ul>
                </div>
              </div> <!-- class="box" -->
            </div> <!-- class="large-4 medium-6 small-12 columns" -->
          </div> <!-- id="backlog-region" -->
        </div>
      </section>

