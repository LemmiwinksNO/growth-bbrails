      <section>
        <div class="row large-12">

          <div id="panel-region" class="large-12 columns">
            <div class="left">
              <button id="new-notdo" class="button radius tiny secondary">
                <i class="fi-plus"></i>
                Add Item
              </button>
              <button id="new-project" class="button radius tiny secondary">
                <i class="fi-plus"></i>
                Add Project
              </button>
              <button id="select-project" data-dropdown="drop" class="button radius tiny secondary dropdown">
                <i class="fi-plus"></i>
                Select Project
              </button>
              <ul id="drop" data-dropdown-content class="f-dropdown">
                <li><a>This is a link</a></li>
                <li><a>This is another</a></li>
                <li><a>Yet another</a></li>
              </ul>
              <button id="select-project" data-dropdown="drop2" class="button radius tiny secondary dropdown">
                <i class="fi-plus"></i>
                Select Project
              </button>
              <ul id="drop2" data-dropdown-content class="f-dropdown">
                <li>This is a link</li>
                <li>This is another</li>
                <li>Yet another</li>
              </ul>
            </div>
          </div> <!-- id="panel-region" -->

          <div id="new-region" class="large-12 columns">
            <form data-type="new">
              <div class="panel radius">
                <div id="new-notdo">
                  <div class="row">
                    <div class="large-12 columns">
                      <label>What's the task?</label>
                      <input type="text" placeholder="Task Name" />
                    </div>
                  </div>
                </div>
                <footer>
                  <ul class="inline-list">
                    <li>
                      <button data-form-button="primary" class="button tiny radius">Save</button>
                    </li>
                    <li>
                      <button data-form-button="cancel" class="button tiny secondary radius">Cancel</button>
                    </li>
                  </ul>
                </footer>
              </div>
            </form>
          </div> <!-- id="new-region" -->

          <div id="new-region" class="large-12 columns">
            <form data-type="new">
              <div class="panel radius">
                <div id="new-notdo">
                  <div class="row">
                    <div class="large-12 columns">
                      <label>What's the project?</label>
                      <input type="text" placeholder="Project Name" />
                    </div>
                  </div>
                </div>
                <footer>
                  <ul class="inline-list">
                    <li>
                      <button data-form-button="primary" class="button tiny radius">Save</button>
                    </li>
                    <li>
                      <button data-form-button="cancel" class="button tiny secondary radius">Cancel</button>
                    </li>
                  </ul>
                </footer>
              </div>
            </form>
          </div> <!-- id="new-region" -->

        </div> <!-- class="row large-12 columns" -->
      </section>

      <section>
        <div class="row">
          <div id="backlog-region">
